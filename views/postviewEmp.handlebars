<div class="container user">
  <div class="row">
    <div class="col-sm-4 sideNav">
      <a href="/findUser" type="button" class="btn btn-primary profile" data-id="{{id}}">My Profile</a>
      <br>
      <a href="/findUserPost" type="button" class="btn btn-primary feedback" data-id="{{id}}">My Feedback</a>
      <br>
      <a href="/api/getQuestions" type="button" class="btn btn-primary questions" data-id="{{id}}">Questions</a>
      <br>
      <a href="/logout" type="button" class="btn btn-primary questions" data-id="{{id}}">Logout</a>
    </div>
    <div class="col-sm-8 display">
      <h1>Post</h1>
      <br>
      {{#each post}}
      <h4>Name: {{'User.name'}}</h4>
      <h6>Subject: {{subject}}</h6>
      <p>{{content}}</p>
      <br>
      {{/each}}
    </div>
  </div>
</div>
<div class="container user">
  <div>
    <h3>New Message</h3>
    <form id="newMessage" data-id="{{id}}">
      <div class="form-group">
        <label for="formSubject">Subject</label>
        <input class="form-control" id="formSubject" aria-describedby="emailHelp">
      </div>
      <div class="form-group">
        <label for="formCategory">Category</label>
        <select class="form-control" name="category" id="formCategory">
          <option value="management">management</option>
          <option value="hours">hours</option>
          <option value="coworkers">coworkers</option>
          <option value="other">coworkers</option>
        </select>
      </div>
      <div class="form-group">
        <label for="formMessage">Message</label>
        <textarea class="form-control" id="formMessage" rows="3"></textarea>
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>
</div>